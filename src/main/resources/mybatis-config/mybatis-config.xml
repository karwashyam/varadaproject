<?xml version="1.0" encoding= "UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<settings>
		<setting name="useGeneratedKeys" value="true" />
	</settings>
	<typeAliases>
		<typeAlias alias="UserModel" type="com.webapp.models.User" />
		<typeAlias alias="JQTableUtils" type="com.fnf.utils.JQTableUtils" />
  		<typeAlias alias="State" type="com.webapp.models.State" />
  		<typeAlias alias="StateDto" type="com.webapp.dto.StateDto" />
  		
		<typeAlias alias="CityDto" type="com.webapp.dto.CityDto" />
		<typeAlias alias="CityModel" type="com.webapp.models.CityModel" />
		
		<typeAlias alias="EmployeeModel" type="com.webapp.models.EmployeeModel" />
		<typeAlias alias="EmployeeDto" type="com.webapp.dto.EmployeeDto" />
		
		<typeAlias alias="ProjectDto" type="com.webapp.dto.ProjectDto" />
		
  		<typeAlias alias="ProjectModel" type="com.webapp.models.ProjectModel" />
  		<typeAlias alias="ProjectPlotsModel" type="com.webapp.models.ProjectPlotsModel" />
  		<typeAlias alias="PaymentSchemeModel" type="com.webapp.models.PaymentSchemeModel" />
  		<typeAlias alias="PaymentSchemeDto" type="com.webapp.dto.PaymentSchemeDto" />
  		<typeAlias alias="ProjectPaymentSchemeDto" type="com.webapp.dto.ProjectPaymentSchemeDto" />
  		<typeAlias alias="ProjectPaymentSchemeModel" type="com.webapp.models.ProjectPaymentSchemeModel" />
  		
	</typeAliases>

	<mappers>
		
		<mapper resource="mybatis-config/User.xml" /> 
		<mapper resource="mybatis-config/Session.xml" />
  		<mapper resource="mybatis-config/Login.xml" />
  		<mapper resource="mybatis-config/State.xml" />
  		<mapper resource="mybatis-config/City.xml" />

  		<mapper resource="mybatis-config/Employee.xml" />
  		<mapper resource="mybatis-config/Project.xml" />
  		<mapper resource="mybatis-config/PaymentScheme.xml" />
  		<mapper resource="mybatis-config/ProjPaymentScheme.xml" />
  		
	</mappers>
</configuration>